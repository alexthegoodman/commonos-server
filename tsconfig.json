{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "ES2015",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": ["*.ts", "**/*.ts"],
  "exclude": ["dist", "seed", "prisma", "node_modules"]
}
